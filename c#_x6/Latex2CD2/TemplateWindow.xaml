<Window x:Class="Latex2CD2.TemplateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TemplateWindow" Height="478" Width="569">
    <DockPanel HorizontalAlignment="Left" Height="448" LastChildFill="False" VerticalAlignment="Top" Width="561">
        <StackPanel DockPanel.Dock="Bottom" Height="43" Orientation="Horizontal" Margin="0,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button x:Name="SaveButton" Content="Save" Width="75" Padding="10,5" Margin="10,5" Click="SaveButton_Click"/>
            <Label Content="%%ANCHOR%% for input location" VerticalContentAlignment="Center"/>
            <Button x:Name="DefaultButton" Content="Default" Width="75" Padding="10,5" Margin="10,5" Click="DefaultButton_Click"/>
        </StackPanel>
        <TextBox x:Name="TemplateText" Height="398" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" DockPanel.Dock="Top" Margin="0,0,0,0" AcceptsReturn="True" AcceptsTab="True"/>
    </DockPanel>

</Window>
